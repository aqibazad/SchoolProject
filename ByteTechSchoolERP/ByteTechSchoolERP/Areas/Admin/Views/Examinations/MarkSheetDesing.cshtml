<div class="page-wrapper">
    <div class="tab-container container-fluid">
        <div class="col-md-12">
            <div class="style-0" style="max-width: 98%; margin-left: 5px;">
                <strong>Classes</strong> | <i class="fas fa-home home-icon"></i> - Add New Class
            </div>
        </div>
    </div>
    <div  style="margin-left: 20px; margin-top:20px !important;">

        <div class="row">
            <!-- Form Section -->
            <div class="col-md-4">
                <form method="post" enctype="multipart/form-data" asp-action="CreateExam" asp-controller="Examinations" asp-area="Admin">
                    <div class="form-group">
                        <label for="templateName">Template Name</label>
                        <input type="text" class="form-control" id="templateName" name="TemplateName">
                    </div>
                    <div class="form-group">
                        <label for="examName">Exam Name</label>
                        <input type="text" class="form-control" id="examName" name="ExamName">
                    </div>
                    <div class="form-group">
                        <label for="schoolName">School Name</label>
                        <input type="text" class="form-control" id="schoolName" name="SchoolName">
                    </div>
                    <div class="form-group">
                        <label for="examCenter">Exam Center</label>
                        <input type="text" class="form-control" id="examCenter" name="ExamCenter">
                    </div>
                    <div class="form-group">
                        <label for="bodyText">Body Text</label>
                        <textarea class="form-control" id="bodyText" name="BodyText"></textarea>
                    </div>
                    <div class="form-group">
                        <label for="footerText">Footer Text</label>
                        <textarea class="form-control" id="footerText" name="FooterText"></textarea>
                    </div>
                    <div class="form-group">
                        <label for="printingDate">Printing Date</label>
                        <input type="date" class="form-control" id="printingDate" name="PrintingDate">
                    </div>
                    <!-- File Uploads -->
                    <div class="form-group">
                        <label for="headerImage">Header Image</label>
                        <input type="file" class="form-control-file" id="headerImage" name="HeaderImage">
                    </div>
                    <div class="form-group">
                        <label for="leftLogo">Left Logo Image</label>
                        <input type="file" class="form-control-file" id="leftLogo" name="LeftLogo">
                    </div>
                    <div class="form-group">
                        <label for="rightLogo">Right Logo Image</label>
                        <input type="file" class="form-control-file" id="rightLogo" name="RightLogo">
                    </div>
                    <div class="form-group">
                        <label for="leftSign">Left Sign</label>
                        <input type="file" class="form-control-file" id="leftSign" name="LeftSign">
                    </div>
                    <div class="form-group">
                        <label for="middleSign">Middle Sign</label>
                        <input type="file" class="form-control-file" id="middleSign" name="MiddleSign">
                    </div>
                    <div class="form-group">
                        <label for="rightSign">Right Sign</label>
                        <input type="file" class="form-control-file" id="rightSign" name="RightSign">
                    </div>
                    <div class="form-group">
                        <label for="backgroundImage">Background Image</label>
                        <input type="file" class="form-control-file" id="backgroundImage" name="BackgroundImage">
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>

            <!-- DataTable Section -->
            <div class="col-md-8">
                <table id="myTable" class="display">
                    <thead>
                        <tr>
                            <th>Template Name</th>
                            <th>Exam Name</th>
                            <th>School Name</th>
                            <th>Exam Center</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr class="clickable-row"
                                data-headerimage="@item.HeaderImageUrl"
                                data-leftlogo="@item.LeftLogoUrl"
                                data-rightlogo="@item.RightLogoUrl"
                                data-leftsign="@item.LeftSignUrl"
                                data-middlesign="@item.MiddleSignUrl"
                                data-rightsign="@item.RightSignUrl"
                                data-backgroundimage="@item.BackgroundImageUrl"
                                data-templatename="@item.TemplateName"
                                data-examname="@item.ExamName"
                                data-schoolname="@item.SchoolName"
                                data-examcenter="@item.ExamCenter"
                                data-bodytext="@item.BodyText"
                                data-footertext="@item.FooterText"
                                data-printingdate="@item.PrintingDate">

                                <td>@item.TemplateName</td>
                                <td>@item.ExamName</td>
                                <td>@item.SchoolName</td>
                                <td>@item.ExamCenter</td>
                          
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

